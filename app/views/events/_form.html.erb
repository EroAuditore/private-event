<%= form_with(model: event) do |form| %>
  

  <div class="field">
    <%= form.label :description %>
    <%= form.text_area :description %>
  </div>

  <div class="field">
    <%= form.label :event_date %>
    <%= form.date_select :event_date %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>



